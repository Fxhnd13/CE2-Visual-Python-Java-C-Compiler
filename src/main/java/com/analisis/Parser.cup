                                        |CASE FLOTANTE
                                        |CASE CARACTER
                                        |DEFAULT
                                        |error:e {: agregarError("Sintactico",(String)e,"Se esperaba un valor para un caso de un switch.",eleft,eright); :}
                                        ;

